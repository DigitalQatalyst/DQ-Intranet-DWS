{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|json|pdf|mp4|webp|avif))",
            "destination": "/$1"
        },
        {
            "source": "/images/(.*)",
            "destination": "/images/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}
